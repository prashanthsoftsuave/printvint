{
    "wod": {
        "enable-redirection-to-converter": true,
        "enable-redirection-to-webp-realizer": true,
        "base-htaccess-on-these-capability-tests": {
            "passThroughHeaderWorking": false,
            "passThroughEnvWorking": false,
            "modHeaderWorking": false
        },
        "destination-extension": "append",
        "destination-folder": "separate",
        "forward-query-string": true,
        "image-roots": [
            {
                "id": "uploads",
                "rel-path": "wp-content/uploads",
                "url": "https://diamanti.com/wp-content/uploads"
            },
            {
                "id": "themes",
                "rel-path": "wp-content/themes",
                "url": "https://diamanti.com/wp-content/themes"
            },
            {
                "id": "plugins",
                "rel-path": "wp-content/plugins",
                "url": "https://diamanti.com/wp-content/plugins"
            },
            {
                "id": "wp-content",
                "rel-path": "wp-content",
                "url": "https://diamanti.com/wp-content"
            },
            {
                "id": "index",
                "rel-path": ".",
                "url": "https://diamanti.com"
            }
        ],
        "success-response": "converted"
    },
    "webp-convert": {
        "serve-image": {
            "headers": {
                "cache-control": false,
                "content-length": true,
                "content-type": true,
                "expires": false,
                "last-modified": true
            }
        },
        "fail": "original",
        "convert": {
            "converters": [
                {
                    "converter": "vips",
                    "options": {
                        "smart-subsample": false,
                        "preset": "none"
                    }
                },
                {
                    "converter": "gd",
                    "options": {
                        "png": {
                            "skip": false
                        }
                    }
                },
                {
                    "converter": "imagick",
                    "options": []
                },
                {
                    "converter": "graphicsmagick",
                    "options": {
                        "use-nice": true
                    }
                }
            ],
            "jpeg": {
                "encoding": "auto",
                "quality": "auto",
                "default-quality": 70,
                "max-quality": 100,
                "near-lossless": 60
            },
            "png": {
                "encoding": "auto",
                "quality": 85,
                "near-lossless": 60,
                "alpha-quality": 80
            },
            "metadata": "none",
            "log-call-arguments": true
        }
    }
}